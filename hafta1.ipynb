    "s = 0\n",
    "def power(x,n):\n",
    "    global s\n",
    "    s = s + 1\n",
    "    if(n==0):\n",
    "        return 1\n",
    "    if(n==1):\n",
    "        return x\n",
    "    if(n%2==0):\n",
    "        return power(x*x,n/2)\n",
    "    else:\n",
    "        return power(x*x,n/2)*x"
